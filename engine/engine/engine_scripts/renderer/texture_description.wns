class TextureDescription {
    CString name = "";
    Int width = 0;
    Int height = 0;
    Int format = format_max;

    CString get_name() {
        return name;
    }

    Int get_width() {
        return width;
    }

    Int get_height() {
        return height;
    }

    Int get_format() {
        return format;
    }
}

// CHECK: {{.*void_func.*\{}}
// RUN: void_func -> 3
Int void_func() {
// CHECK-NEXT: int32_t _return;
    return 3;
// CHECK-NEXT: _return = 3;
// CHECK-NEXT: return _return;
}

// CHECK: {{.*b_func.*\{}}
// RUN: b_func -> 3
Int b_func() {
// CHECK: enter1 = true;
    if (true) {
        return 3;
    }
    return 4;
}

// CHECK: {{.*ff.*\{}}
// RUN: ff -> 3
Int ff() {
// CHECK: enter3 = (3.0f < 4.0f);
    if (3.0 < 4.0) {
        return 3;
    }
    return 4;
}

    log_error().o("Hello Main2 ").o(x.get_param()).end();
    return x.get_param();
}

@Verb("{\"name\":\"do_thing2\", \"description\": \"Do the thing2\", \"function\": \"main3\", \"verb\": [\"do_thing\"]}")

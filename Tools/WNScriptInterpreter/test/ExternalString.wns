Int function test8(){
	Char[] c = Char[10000];
	Int pos = 0;
	for(Int i = 0; i < 100; i = i + 1;){
		pos = pos + appendToBuff(c, pos, i / 3.0, 5, 5);
		if((i % 10) == 9){
			pos = pos + appendToBuff(c, pos, '\n');
		} else {
		    pos = pos + appendToBuff(c, pos, " .. ", 5);
		}
	}
	writeString(c, pos);
	return(#c);
}

